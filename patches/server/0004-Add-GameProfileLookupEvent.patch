+                    }
+		    }
+		    // Yatopia end
             }
-            if (this.profile.isComplete()) {
-                server.getUserCache().saveProfile(this.profile);
-            }
-        }
         return profile.isComplete() && (!onlineMode || !textures || hasTextures());
     }
 
